{
  "name": "fourier_fun_time",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "react-native": "./node_modules/.bin/react-native",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "bundle_ios": "npx react-native bundle --entry-file='index.js' --bundle-output='./ios/pak/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "bundle_android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundles": "npm run bundle_ios && npm run bundle_android",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleQaRelease && ./gradlew installQaRelease",
    "android:installDebug": "cd android && ./gradlew assembleQaDebug && ./gradlew installQaDebug"
  },
  "dependencies": {
    "@react-native-community/cli": "^4.12.0",
    "@react-native-community/cli-platform-ios": "^4.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/toolbar-android": "^0.1.0-rc.1",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "@types/deep-equal": "^1.0.1",
    "@types/react-redux": "^7.1.9",
    "deep-equal": "^2.0.4",
    "prop-types": "^15.7.2",
    "react": "16.11.0",
    "react-native": "^0.62.2",
    "react-native-fast-image": "^8.3.2",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-permissions": "^2.2.2",
    "react-native-reanimated": "^1.13.0",
    "react-native-redash": "^14.2.4",
    "react-native-safe-area-context": "^3.1.7",
    "react-native-screens": "^2.10.1",
    "react-native-svg": "^12.1.0",
    "react-native-vector-icons": "^6.6.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "react-native-chart-kit": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/jest": "^24.0.24",
    "@types/react-native": "^0.62.0",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.58.0",
