    "react-test-renderer": "16.11.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
